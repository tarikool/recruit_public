{
    "name": "laravel/laravel",
    "type": "project",
    "description": "QuickRecruit CRM",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-dompdf": "^2.0",
        "barryvdh/laravel-translation-manager": "^0.6.5",
        "creativeorange/gravatar": "^1.0",
        "doctrine/dbal": "^3.6.5",
        "google/recaptcha": "^1.2",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/support": "^10.16.1",
        "intervention/image": "^2.5",
        "jaybizzle/laravel-crawler-detect": "^1.2",
        "jeremykenedy/laravel-exception-notifier": "^2.2",
        "jeremykenedy/laravel-roles": "^10.0",
        "jeremykenedy/uuid": "^3.1",
        "laravel/framework": "^10.0",
        "laravel/helpers": "^1.4",
        "laravel/passport": "^11.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "laravel/vonage-notification-channel": "^3.0",
        "laravelcollective/html": "^6.4",
        "psr/http-factory": "^1.0",
        "pusher/pusher-php-server": "^7.2",
        "sentry/sentry-laravel": "3.*",
        "spatie/laravel-html": "^3.2",
        "srmklive/paypal": "~3.0",
        "stripe/stripe-php": "^7.0.0",
        "symfony/http-client": "^6.1",
        "symfony/mailgun-mailer": "^6.1",
        "symfony/postmark-mailer": "^6.1",
        "yajra/laravel-datatables-oracle": "~10.0",
        "yoeunes/toastr": "^2.3"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^2.0" ,
        "laracasts/utilities": "^3.1",
        "mews/purifier": "^3.3",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "rachidlaasri/laravel-installer": "^4.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Helpers/helper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"

        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
